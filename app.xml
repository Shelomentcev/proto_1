  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <pathelement location="${javac2.home}/javac2.jar"/>
    <pathelement location="${javac2.home}/jdom.jar"/>
    <pathelement location="${javac2.home}/asm4-all.jar"/>
    <pathelement location="${javac2.home}/jgoodies-forms.jar"/>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  
  <!-- Module proto_1 -->
  
  <dirname property="module.proto_1.basedir" file="${ant.file}"/>
  
  
  <property name="module.jdk.home.proto_1" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.proto_1" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.proto_1" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.proto_1" value="${compiler.args}"/>
  
  <property name="proto_1.output.dir" value="${module.proto_1.basedir}/out/production/proto_1"/>
  <property name="proto_1.testoutput.dir" value="${module.proto_1.basedir}/out/test/proto_1"/>
  
  <path id="proto_1.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="proto_1.module.production.classpath">
    <path refid="${module.jdk.classpath.proto_1}"/>
  </path>
  
  <path id="proto_1.runtime.production.module.classpath">
    <pathelement location="${proto_1.output.dir}"/>
  </path>
  
  <path id="proto_1.module.classpath">
    <path refid="${module.jdk.classpath.proto_1}"/>
    <pathelement location="${proto_1.output.dir}"/>
  </path>
  
  <path id="proto_1.runtime.module.classpath">
    <pathelement location="${proto_1.testoutput.dir}"/>
    <pathelement location="${proto_1.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.proto_1">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.proto_1">
    <patternset refid="excluded.from.module.proto_1"/>
  </patternset>
  
  <path id="proto_1.module.sourcepath">
    <dirset dir="${module.proto_1.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.proto_1" depends="compile.module.proto_1.production,compile.module.proto_1.tests" description="Compile module proto_1"/>
  
  <target name="compile.module.proto_1.production" depends="register.custom.compilers" description="Compile module proto_1; production classes">
    <mkdir dir="${proto_1.output.dir}"/>
    <javac2 destdir="${proto_1.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.proto_1}/javac">
      <compilerarg line="${compiler.args.proto_1}"/>
      <bootclasspath refid="proto_1.module.bootclasspath"/>
      <classpath refid="proto_1.module.production.classpath"/>
      <src refid="proto_1.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.proto_1"/>
    </javac2>
    
    <copy todir="${proto_1.output.dir}">
      <fileset dir="${module.proto_1.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.proto_1.tests" depends="register.custom.compilers,compile.module.proto_1.production" description="compile module proto_1; test classes" unless="skip.tests"/>
  
  <target name="clean.module.proto_1" description="cleanup module">
    <delete dir="${proto_1.output.dir}"/>
    <delete dir="${proto_1.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.proto_1" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.proto_1" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
